    {
      "name": "MathModule",
      "description": "Module for solving math tasks",
      "type": "Module",
      "impl": "moduls.math_module.MathModule",
      "intents": [
        {
          "name": "Welcome",
          "samples": [
            "Help me solve my math homework",
            "Let's do math",
            "Solve an equation for me",
            "Do some math for me",
            "I need your help with math",
            "Solve for me the next math task",
            "Solve for me equation"
          ]
        },
        {
          "name": "Solving mathematical tasks",
          "key_phrases":[
            "math",
            "calculate",
            "solve"
          ],
          "samples": [
            "I want to calculate",
            "Help me to calculate",
            "calculate 1 + 5",
            "calculate 2 - 6",
            "calculate 3 * 7",
            "calculate 4 / 8",
            "calculate 0 = 9"
          ],
          "parameters": [
            {
              "name": "Request",
              "data_type": "str",
              "obligatory": true,
              "question": "What would you like to solve?",
              "regexp": "(.*math (.+)$)|(.*calculate (.+)$)|(.*solve (.+)$)",
              "reqexp_group_id": [1,3,5]
            }
          ]
        }
      ]
    },
