{
  "applications": [
    {
      "name": "Calendar",
      "description": "App for calendar management",
      "type": "Module",
      "intents": [
        {
          "name": "Create remind",
          "key_phrases": [
            "remind"
          ],
          "parameters": [
            {
              "name": "Date",
              "data_type": "Date",
              "obligatory": true,
              "question": "When should I remind you?"
            },
            {
              "name": "Subject",
              "data_type": "str",
              "obligatory": true,
              "question": "What should I remind you?",
               "regexp": ".* remind .* to (.+)"
            }
          ]
        }
      ]
    },
    {
      "name": "Home",
      "description": "App for home management",
      "type": "RemoteApp",
      "url": "http://127.0.0.1:7878",
      "intents": [
        {
          "name": "Set temperature",
          "key_phrases": [
            "set",
            "set up"
          ],
          "parameters": [
            {
              "name": "temperature",
              "data_type": "Number",
              "obligatory": true,
              "question": "What temperature I should setup?"
            }
          ]
        },
        {
          "name": "Turn on",
          "key_phrases": [
            "Turn on"
          ],
          "parameters": [
            {
              "name": "object",
              "data_type": "str",
              "obligatory": true,
              "question": "What should I turn on?",
              "regexp": ".* turn on ([a-zA-Z]+)"
            }
          ]
        },
        {
          "name": "Turn off",
          "key_phrases": [
            "Turn off"
          ],
          "parameters": [
            {
              "name": "object",
              "data_type": "str",
              "obligatory": true,
              "question": "What should I turn off?",
              "regexp": ".* turn off ([a-zA-Z]+)"
            }
          ]
        },
        {
          "name": "Show dashboard",
          "key_phrases": [
            "Show"
          ],
          "parameters": [
            {
              "name": "sensor",
              "data_type": "str",
              "obligatory": true,
              "question": "What sensor do you want to check?",
              "regexp": ".* show ([a-zA-Z]+)"
            }
          ]
        }
      ]
    }
  ]
}